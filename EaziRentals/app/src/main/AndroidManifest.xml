<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eazi.com.eazirentals">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Home" />
        <activity android:name=".SelectBike" />
        <activity android:name=".BookingHistory" />
        <activity android:name=".Tariff" />
        <activity android:name=".ReferAndEarn" />
        <activity android:name=".Offers" />
        <activity android:name=".AboutUs" />
        <activity android:name=".ContactUs" />
        <activity android:name=".Faq" />
        <activity android:name=".Cart"></activity>
    </application>

</manifest>